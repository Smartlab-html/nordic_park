mixin sPlace(data)
	// start sPlace
	+b.SECTION.sPlace.section#sPlace&attributes(attributes)
		.container
			+breadcrumb-light(`Главная`,`Расположение`)
			h1 Расположение
			+e.caption.animate__animated.animate__fadeInUp Коттеджный поселок расположен в&nbsp;одном из&nbsp;самых экологически чистых мест Подмосковья&nbsp;&mdash; в&nbsp;20&nbsp;км от&nbsp;МКАД по&nbsp;Дмитровскому шоссе, в&nbsp;окружении реликтового леса.
			+e.down-link.animate__animated.animate__fadeInUp.scroll-link( href="#sPlace__map")
				+icon(`arrow-down`)
		
	// end sPlace

mixin sMap
	+b.sMap#sPlace__map
		+sMapBody
mixin sMapBody
		<link rel="stylesheet" href="https://unpkg.com/leaflet@1.7.1/dist/leaflet.css" crossorigin=""/>
		<script src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js" crossorigin=""></script>
		style.
			img[src*="/logo_nordic.svg"] {border: 1px solid transparent;border-radius: 50%;z-index: 999 !important;}
			.obj_tooltip {border: 1px solid transparent;border-radius: 5px 5px 5px 5px; positon: absolute;top: -35px;left: 12px;}
			.obj_tooltip.tooltip_b {top: unset;bottom: -60px;}
			.main_tooltip {border: 1px solid transparent;border-radius: 5px 5px 5px 5px; positon: absolute;top: -45px;left: 22px;}
			.obj_tooltip:before, .main_tooltip:before {display: none}
			#map1 .leaflet-top {top: 50%;transform: translateY(-50%);}
			.leaflet-touch .leaflet-bar {border: none;}
			.leaflet-touch .leaflet-bar a {width: 40px;height: 40px;line-height: 40px;border-radius: 50% !important;color: #777 !important;}
			.leaflet-touch .leaflet-bar a:first-child {margin-bottom: 20px;}
			.leaflet-touch .leaflet-bar a:hover {transform: scale(1.1);} 
		+e.map-widget#map1
		script.
			let mobile=0;
			var map = L.map('map1', {scrollWheelZoom: false, zoomControl: false}).setView([56.097619, 37.59], 11),
			cities = L.layerGroup([
				L.marker([56.093817, 37.540344],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild3237-3665-4536-b435-613666303736/hotel-05.svg',iconSize: [50, 50]})}).bindTooltip('Arthurs Spa Hotel', {className: 'obj_tooltip tooltip_b'}),
				L.marker([56.097221, 37.611527],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild6134-3162-4161-b636-613664633134/golf-02.svg',iconSize: [50, 50]})}).bindTooltip('Гольф и яхт-клуб <br>Пестово', {className: 'obj_tooltip'}),
				L.marker([56.055794, 37.531325],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild3366-3764-4962-a139-303133393333/shopping-02.svg',iconSize: [50, 50]})}).bindTooltip('Торговый центр <br>Каштановая роща', {className: 'obj_tooltip'}),		
				L.marker([56.085168, 37.635991],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild6261-6265-4533-b634-653164663162/school-01.svg',iconSize: [50, 50]})}).bindTooltip('Ломоносовская школа', {className: 'obj_tooltip'}),
				L.marker([56.129879, 37.540571],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild6436-3634-4263-b861-303665633235/boat-04.svg',iconSize: [50, 50]})}).bindTooltip('Икша Ривер Клаб', {className: 'obj_tooltip'})
			]);
			L.tileLayer('https://tile{s}.maps.2gis.com/tiles?x={x}&y={y}&z={z}&ts='+(mobile?'online_hd':'online_sd'),{subdomains:['0','1','2','3','4']}).addTo(map);
			L.control.zoom({zoomInTitle:'Увеличить', zoomOutTitle:'Уменьшить', position: 'topright'}).addTo(map);
			map.attributionControl.setPrefix('').addAttribution('&copy; <a href="http://www.2gis.ru" target="_blank" title="2ГИС — Городской информационный сервис">2ГИС</a> | &copy; <a href="https://www.openstreetmap.org/copyright" target="_blank">OpenStreetMap</a> | <a href="https://leafletjs.com/" target="_blank">Leaflet</a>');
			L.marker([56.097619, 37.548508],{icon:L.icon({iconUrl: 'https://static.tildacdn.com/tild3331-3338-4965-a235-356265373034/logo_nordic.svg',iconSize: [75, 75]})}).bindTooltip('Коттеджный поселок <br>Nordic Park', {className: 'main_tooltip'}).addTo(map);
				cities.addTo(map); 
		<!- /Map ->
		//- script(src='https://api-maps.yandex.ru/2.1/?lang=ru_RU&apikey=6331e0a6-b922-4b24-a8f0-88e62d68142f')
		//- +e.map-widget#map-place
		

mixin sMapContacts
	+b.sMap.sMapContacts#sContacts__map
		+sMapBody

mixin sContacts(data)
	// start sPlace
	+b.SECTION.sPlace.sContacts.section#sPlace&attributes(attributes)
		.container
			+breadcrumb-light(`Главная`,`Контакты`)
			h1 Контакты
			.row
				mixin item(title,caption)
					+e.title!=title
					+e.caption-info!=caption
				mixin item2()
					+e.title Контактный телефон
					+e.caption-info(href="tel:+79851405500") +7 985 140-55-00
				mixin item3()
					+e.title E-MAIL
					+e.caption-info(href="mailto:info@nordicpark.ru") info@nordicpark.ru
				.col-lg-3.col-sm-6.animate__animated.animate__fadeIn.animate__slow.animate__delay-1s(style=`--animate-duration: 800ms; --animate-delay:0.5s;`)
					+item(`ОФИС ПРОДАЖ`,`г. Москва, Дмитровское шоссе, 161А`)
				.col-lg-9.col-sm-6.animate__animated.animate__fadeIn.animate__slow.animate__delay-1s(style=`--animate-duration: 800ms; --animate-delay:1s;`)
					+item(`Адрес поселка`,`городской округ Мытищи, д. Лысково`)
				.col-lg-3.col-sm-6.animate__animated.animate__fadeIn.animate__slow.animate__delay-1s(style=`--animate-duration: 800ms; --animate-delay:1.5s;`)
					+item2
				.col-lg-9.col-sm-6.animate__animated.animate__fadeIn.animate__slow.animate__delay-1s(style=`--animate-duration: 800ms; --animate-delay:2s;`)
					+item3
			+e.down-link.animate__animated.animate__fadeInUp.scroll-link(data-wow-duration="1s" data-wow-delay="0s" href="#sContacts__map")
				+icon(`arrow-down`)
	// end sPlace