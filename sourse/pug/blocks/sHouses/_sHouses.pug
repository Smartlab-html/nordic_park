mixin house-item(title,square,img,delay)
	+b.house-item.bg-wrap.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" data-wow-delay=`${delay}.0s` style=`--animate-duration: 400ms; --animate-delay:${delay}.0s;`)
		+pic(`house-item-${img}.jpg`,``,0).picture-bg
		h3.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 200ms; --animate-delay:${delay}.4s;`)!=title
		+e.square.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 200ms; --animate-delay:${delay}.6s;`)
			+e.square-info!=square
			+e.square-title Общая площадь, м²
		+e.more.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 200ms; --animate-delay:${delay}.8s;` href="#")
			span Подробнее
			+icon(`chevron-right`)

