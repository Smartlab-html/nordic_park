mixin sHouses(data)
	// start sHouses
	+b.SECTION.sHouses.section#sHouses&attributes(attributes)
		.container
			+breadcrumb(`ГЛАВНАЯ`,`ДОМА`)
			+e.title-wrap.animate__animated.animate__fadeIn.animate__delay-1s
				h1 Дома
			.row
				.col-lg-6
					+b.house-item.bg-wrap.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 400ms; --animate-delay:0.0s;`)
						+pic(`house-item-1.jpg`,``,0).picture-bg
						h3.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 200ms; --animate-delay:0.4s;`) VIND
						+e.square.wow.animate__slow.animate__delay-1s(data-animate="animate__zoomIn" style=`--animate-duration: 200ms; --animate-delay:0.6s;`)
